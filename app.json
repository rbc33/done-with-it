{
  "expo": {
    "name": "done-with-it",
    "slug": "done-with-it",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rmc33.done_with_it",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "iosDisplayInForeground": true
        }
      ],
      "expo-secure-store"
    ],
    "owner": "rbc33"
  
  }
}
